{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "js/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "imgs/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/health",
            "dest": "/api/health"
        },
        {
            "src": "/api/products",
            "dest": "/api/products"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/imgs/(.*)",
            "dest": "/imgs/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}