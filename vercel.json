{
    "version": 2,
    "name": "8een-store",
    "builds": [{
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}